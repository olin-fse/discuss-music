    "jasmine": "~3.1.0",
    "jest": "~22.2.2",
    "nodemon": "~1.17.3",
    "supertest": "~3.0.0",
    "wdio-chromedriver-service": "~0.1.2",
    "wdio-jasmine-framework": "~0.3.2",
